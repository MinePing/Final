INSERT INTO member_preference (preference_no, user_no, mbti, preferenced_color, preferenced_location, space_mood, important_factor, leisure_activity, accommodation_type, update_date, result_content) 
VALUES (1, 1, 'ENTJ', 'achromatic', 'city', 'modern', 'work_efficiency', 'social_interaction', 'clean_convenient', '2024-12-15', '리더십이 강하고 효율성을 중시하는 당신은 도시적이고 모던한 환경에서 최고의 퍼포먼스를 발휘합니다. 깔끔하고 편리한 숙소에서 동료들과의 네트워킹을 통해 새로운 아이디어를 얻으세요.');

INSERT INTO member_preference (preference_no, user_no, mbti, preferenced_color, preferenced_location, space_mood, important_factor, leisure_activity, accommodation_type, update_date, result_content) 
VALUES (2, 2, 'ISFJ', 'green_brown', 'mountain', 'eco_friendly', 'rest_area', 'meditation_yoga_rest', 'nature_lodging', '2024-12-14', '따뜻하고 배려심 많은 당신은 자연 속에서 진정한 휴식을 찾습니다. 친환경적인 산속 숙소에서 명상과 요가를 통해 마음의 평화를 되찾고 에너지를 충전하세요.');

INSERT INTO member_preference (preference_no, user_no, mbti, preferenced_color, preferenced_location, space_mood, important_factor, leisure_activity, accommodation_type, update_date, result_content) 
VALUES (3, 3, 'ENFP', 'yellow_orange', 'sea', 'urban_nature', 'activity', 'swimming_marine_sports', 'emotional_style', '2024-12-13', '열정적이고 모험을 좋아하는 당신은 바다에서 다양한 액티비티를 즐기며 새로운 경험을 추구합니다. 감성적인 스타일의 숙소에서 해양스포츠를 만끽하며 창의적 영감을 얻으세요.');

INSERT INTO member_preference (preference_no, user_no, mbti, preferenced_color, preferenced_location, space_mood, important_factor, leisure_activity, accommodation_type, update_date, result_content) 
VALUES (4, 4, 'ISTJ', 'blue_sky', 'plain', 'modern', 'work_efficiency', 'reading_walking', 'clean_convenient', '2024-12-12', '체계적이고 신중한 당신은 평온한 평야 지대의 모던한 공간에서 집중력을 발휘합니다. 깨끗하고 편리한 환경에서 독서와 산책을 통해 업무 효율성을 극대화하세요.');

INSERT INTO member_preference (preference_no, user_no, mbti, preferenced_color, preferenced_location, space_mood, important_factor, leisure_activity, accommodation_type, update_date, result_content) 
VALUES (5, 5, 'ESFP', 'purple', 'river', 'camping', 'overall_vibe', 'local_exploration_food', 'shared_space', '2024-12-11', '사교적이고 즐거움을 추구하는 당신은 강가의 캠핑 분위기에서 지역 탐험과 맛집 투어를 즐깁니다. 공유 공간에서 새로운 사람들과 만나며 특별한 추억을 만들어보세요.');

INSERT INTO member_preference (preference_no, user_no, mbti, preferenced_color, preferenced_location, space_mood, important_factor, leisure_activity, accommodation_type, update_date, result_content) 
VALUES (6, 6, 'INFP', 'green_brown', 'mountain', 'quiet', 'scenery', 'meditation_yoga_rest', 'nature_lodging', '2024-12-10', '조용하고 성찰적인 당신은 산속의 고요한 환경에서 내면의 목소리에 귀 기울입니다. 자연 숙소에서 아름다운 풍경을 바라보며 명상과 휴식을 통해 창작 영감을 얻으세요.');

INSERT INTO member_preference (preference_no, user_no, mbti, preferenced_color, preferenced_location, space_mood, important_factor, leisure_activity, accommodation_type, update_date, result_content) 
VALUES (7, 7, 'ESFJ', 'blue_sky', 'sea', 'urban_nature', 'rest_area', 'swimming_marine_sports', 'emotional_style', '2024-12-09', '사람을 좋아하고 조화를 중시하는 당신은 바다가 보이는 도시적 자연 공간에서 휴식과 해양 액티비티를 균형있게 즐깁니다. 감성적인 숙소에서 동료들과 소중한 시간을 보내세요.');

INSERT INTO member_preference (preference_no, user_no, mbti, preferenced_color, preferenced_location, space_mood, important_factor, leisure_activity, accommodation_type, update_date, result_content) 
VALUES (8, 8, 'ISTP', 'achromatic', 'plain', 'camping', 'activity', 'local_exploration_food', 'camping_car', '2024-12-08', '실용적이고 독립적인 당신은 평야 지대에서 캠핑카를 이용한 자유로운 여행을 선호합니다. 지역 탐험과 맛있는 음식을 찾아다니며 나만의 페이스로 워케이션을 즐기세요.');

-- mental 테이블 INSERT 문 (모든 필드 포함)
INSERT INTO mental (mental_no, user_no, score, psychological_state, result_content, separation, update_date) 
VALUES (1, 1, 15, 'Normal', '현재 정신건강 상태가 양호합니다. 업무와 개인생활의 균형을 잘 유지하고 있으며, 스트레스 관리도 효과적으로 하고 있습니다. 지속적인 건강한 생활 패턴을 유지하시기 바랍니다.', NULL, '2024-12-15');

INSERT INTO mental (mental_no, user_no, score, psychological_state, result_content, separation, update_date) 
VALUES (2, 2, 28, 'Concern', '약간의 스트레스 징후가 보입니다. 업무 부담이나 대인관계에서 오는 긴장감이 있을 수 있습니다. 충분한 휴식과 취미활동을 통해 스트레스를 해소하시고, 필요시 전문가 상담을 고려해보세요.', 'STRESS', '2024-12-14');

INSERT INTO mental (mental_no, user_no, score, psychological_state, result_content, separation, update_date) 
VALUES (3, 3, 12, 'Normal', '정신적으로 건강한 상태를 유지하고 있습니다. 긍정적인 사고와 활발한 사회활동이 도움이 되고 있습니다. 현재의 라이프스타일을 지속하며 정기적인 자기점검을 해보시기 바랍니다.', NULL, '2024-12-13');

INSERT INTO mental (mental_no, user_no, score, psychological_state, result_content, separation, update_date) 
VALUES (4, 4, 35, 'Mild', '경미한 수준의 심리적 부담을 경험하고 있습니다. 업무 과부하나 개인적 고민이 영향을 주고 있을 수 있습니다. 규칙적인 운동과 명상, 그리고 신뢰할 수 있는 사람과의 대화를 통해 개선해보세요.', 'STRESS', '2024-12-12');

INSERT INTO mental (mental_no, user_no, score, psychological_state, result_content, separation, update_date) 
VALUES (5, 5, 45, 'Severe', '상당한 수준의 번아웃 증상이 나타납니다. 업무에 대한 의욕 저하, 피로감, 무력감 등을 경험하고 있을 수 있습니다. 충분한 휴식과 업무 조정이 필요하며, 전문가의 도움을 받으시길 권합니다.', 'BURNOUT', '2024-12-11');

INSERT INTO mental (mental_no, user_no, score, psychological_state, result_content, separation, update_date) 
VALUES (6, 6, 18, 'Normal', '전반적으로 안정된 정신건강 상태입니다. 자연과의 접촉과 명상 등의 활동이 긍정적인 영향을 주고 있습니다. 현재의 건강한 습관들을 지속하시면서 자기관리에 계속 신경 써주세요.', NULL, '2024-12-10');

INSERT INTO mental (mental_no, user_no, score, psychological_state, result_content, separation, update_date) 
VALUES (7, 7, 32, 'Mild', '경미한 스트레스 상태가 관찰됩니다. 새로운 환경이나 업무 변화에 적응하는 과정에서 오는 긴장감일 수 있습니다. 점진적인 적응과 함께 스트레스 완화 방법을 실천해보시기 바랍니다.', 'STRESS', '2024-12-09');

INSERT INTO mental (mental_no, user_no, score, psychological_state, result_content, separation, update_date) 
VALUES (8, 8, 22, 'Concern', '가벼운 주의가 필요한 상태입니다. 독립적인 성향으로 인해 혼자서 문제를 해결하려 하다가 스트레스가 쌓일 수 있습니다. 적절한 사회적 지지와 도움을 받아들이는 것도 중요합니다.', 'STRESS', '2024-12-08');


